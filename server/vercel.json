{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "MONGO_URL": "@mongo-url",
      "PORT": "8000"
    },
    "functions": {
      "index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  